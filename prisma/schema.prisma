// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model professor {
  id    Int    @id @default(autoincrement())
  nome String
  aluno String
}

model aluno {
  id    Int    @id @default(autoincrement())
  nome String
  aluno String
}

model curso {
  id          Int    @id @default(autoincrement())
  nome        String
  description String
  banner      String
}
